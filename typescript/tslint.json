{
  "rules": {
    "eofline": true,
    "trailing-comma": false,
    "max-line-length": [
      true,
      120
    ],
    "brace-style": [
      true,
      "1tbs",
      {
        "allowSingleLine": false
      }
    ],
    "quotemark": [
      true,
      "single",
      "avoid-template",
      "avoid-escape"
    ],
    "align": [
      true,
      "arguments",
      "parameters",
      "statements",
      "members"
    ],
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "ter-arrow-parens": [
      true,
      "as-needed"
    ],
    "type-literal-delimiter": true,
    "typedef": [
      true,
      "call-signature",
      "parameter",
      "property-declaration",
      "arrow-call-signature"
    ],
    "no-increment-decrement": false
  }
}
